{
  "name": "@microfleet/transport-amqp",
  "description": "microservice utils based on amqp transport layer",
  "version": "0.0.0-development",
  "main": "./lib/amqp.js",
  "scripts": {
    "compile": "babel -d ./lib ./src",
    "lint": "eslint ./src",
    "test": "yarn lint && yarn test:e2e",
    "pretest:e2e": "docker-compose -f ./test/docker-compose.yml up -d",
    "test:e2e": "cross-env NODE_ENV=test nyc mocha",
    "posttest:e2e": "docker-compose -f ./test/docker-compose.yml rm -f -v",
    "prepublishOnly": "yarn compile",
    "bench": "yarn compile && node ./bench/roundtrip.js",
    "semantic-release": "semantic-release",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "preparecommitmsg": "./node_modules/@makeomatic/deploy/git-hooks/prepare-commit-msg $GIT_PARAMS"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 4.x.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/microfleet/transport-amqp.git"
  },
  "author": "Vitaly Aminev <v@makeomatic.ca>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/microfleet/transport-amqp/issues"
  },
  "homepage": "https://github.com/microfleet/transport-amqp#readme",
  "devDependencies": {
    "@makeomatic/deploy": "^6.1.2",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-strict-mode": "^6.24.1",
    "babel-register": "^6.26.0",
    "benchmark": "^2.1.4",
    "chai": "^4.1.1",
    "codecov": "^3.0.2",
    "cross-env": "^5.1.6",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-config-makeomatic": "^2.0.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-mocha": "^5.0.0",
    "eslint-plugin-promise": "^3.8.0",
    "jaeger-client": "^3.10.0",
    "microtime": "^2.1.8",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "sinon": "^5.0.10",
    "stdout-stream": "^1.4.0"
  },
  "peerDependencies": {
    "common-errors": "~1.x.x",
    "stdout-stream": "~1.x.x"
  },
  "dependencies": {
    "@microfleet/amqp-coffee": "^1.1.0",
    "bluebird": "^3.5.1",
    "common-errors": "^1.0.5",
    "debug": "^3.0.1",
    "eventemitter3": "^3.1.0",
    "flatstr": "^1.0.8",
    "hashlru": "^2.2.1",
    "is": "^3.2.1",
    "joi": "^13.3.0",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.17.10",
    "object-hash": "^1.3.0",
    "opentracing": "^0.14.3",
    "pino": "^4.17.3",
    "uuid": "^3.2.1"
  },
  "files": [
    "lib/",
    "src/",
    "yarn.lock"
  ]
